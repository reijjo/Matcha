{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@opencage/geosearch-bundle": "^0.0.18",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "axios": "^0.27.2",
    "flowbite": "^1.5.5",
    "flowbite-react": "^0.3.7",
    "geolib": "^3.3.3",
    "google-maps": "^4.3.3",
    "jquery": "^3.6.3",
    "moment": "^2.29.4",
    "opencage-api-client": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.1.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.5",
    "react-scripts": "5.0.1",
    "react-select": "^5.4.0",
    "react-tag-input": "^6.8.1",
    "shortid": "^2.2.16",
    "socket.io-client": "^4.5.4",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb",
      "airbnb/hooks",
      "plugin:react/jsx-runtime",
      "react-app/jest",
      "plugin:testing-library/react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "use-encapsulation"
    ],
    "rules": {
      "global-require": "off",
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^components",
            "^services",
            "^types"
          ]
        }
      ],
      "jsx-a11y/label-has-associated-control": [
        "error",
        {
          "assert": "either"
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "destructuredArrayIgnorePattern": "^_"
        }
      ],
      "use-encapsulation/prefer-custom-hooks": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/webpack": "^6.1.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-use-encapsulation": "^1.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.4"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack"
  },
  "description": "This 1️⃣ is kind of opinionated. You have been warned. Try it. You might like it.",
  "main": "postcss.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
